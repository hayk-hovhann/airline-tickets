{
     "overrides": [
          {
               "files": ["*.ts", "*.tsx"],
               "options": {
                    "parser": "typescript"
               }
          },
          {
               "files": "*.less",
               "options": {
                    "parser": "less"
               }
          }
     ],
     "semi": true,
     "trailingComma": "all",
     "singleQuote": true,
     "printWidth": 120,
     "tabWidth": 2
}
